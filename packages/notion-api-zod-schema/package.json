{
  "name": "@alanlu-dev/notion-api-zod-schema",
  "type": "module",
  "version": "0.0.0",
  "description": "Zod schemas for the Notion API (unofficial)",
  "keywords": [
    "notion",
    "notion-api",
    "zod",
    "schema",
    "validation"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "pkg:prepare": "pnpm run build",
    "build:clean": "npx rimraf --glob ./dist",
    "build": "unbuild --minify",
    "dev": "unbuild --stub",
    "lint:eslint": "eslint --fix .",
    "lint:prettier": "prettier --ignore-path ../../.prettierignore --write .",
    "lint": "pnpm lint:eslint && pnpm lint:prettier",
    "test": "vitest run"
  },
  "peerDependencies": {
    "zod": "3.22.5"
  },
  "dependencies": {
    "zod": "3.22.5"
  },
  "devDependencies": {
    "@alanlu-dev/eslint-config-base": "workspace:*",
    "@alanlu-dev/tsconfig": "workspace:*",
    "@notionhq/client": "2.2.15",
    "dotenv": "16.4.5",
    "tsx": "4.7.2",
    "zod": "3.22.5"
  }
}
