{
  "name": "@alanlu-dev/nuxt-demo",
  "type": "module",
  "private": true,
  "description": "nuxt demo",
  "license": "MIT",
  "scripts": {
    "build:clean": "npx rimraf --glob ./.output",
    "build": "nuxt build",
    "dev": "nuxt dev --dotenv .env",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "cleanup": "nuxt cleanup",
    "app:prepare": "nuxt prepare",
    "lint:eslint": "eslint --fix .",
    "lint:prettier": "prettier --ignore-path ../../.prettierignore --write .",
    "lint": "pnpm lint:eslint && pnpm lint:prettier",
    "typecheck": "vue-tsc --noEmit",
    "k6": "k6 run k6.mjs",
    "node:dev": "node -r dotenv/config .output/server/index.mjs",
    "node:prod": "node -r dotenv/config .output/server/index.mjs dotenv_config_path=.env.production",
    "pm2:dev": "pm2 start .output/server/index.mjs --name nuxt-fast --node-args='-r dotenv/config'",
    "pm2:config:dev": "pm2 start ecosystem.config.js",
    "pm2:config:prod": "pm2 start ecosystem.config.js --env production",
    "docker:nginx": "docker build -t fast-nginx .",
    "docker:nginx:run": "docker run -d -p 80:80 fast-nginx"
  },
  "dependencies": {
    "@splidejs/splide": "^4.1.4",
    "@splidejs/splide-extension-intersection": "^0.2.0",
    "@vueuse/core": "^10.11.0",
    "class-variant": "^2.0.2",
    "floating-vue": "^5.2.2",
    "i18next": "^23.12.1",
    "maska": "^3.0.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.32",
    "vue-final-modal": "^4.5.4",
    "vue-router": "^4.4.0",
    "vue3-toastify": "^0.2.2",
    "zod": "^3.23.8",
    "zod-i18n-map": "^2.27.0"
  },
  "devDependencies": {
    "@alanlu-dev/nuxt-base": "workspace:*",
    "@formkit/core": "^1.6.5",
    "@formkit/vue": "^1.6.5",
    "@gtm-support/vue-gtm": "^3.0.1",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@nuxt/test-utils": "^3.13.1",
    "@nuxtjs/i18n": "^8.3.1",
    "@nuxtjs/seo": "2.0.0-rc.15",
    "@testing-library/vue": "8.1.0",
    "@vue/test-utils": "^2.4.6",
    "nuxt": "^3.12.3",
    "nuxt-gtag": "^2.0.7",
    "nuxt-security": "^1.4.3",
    "nuxt-splide": "^1.1.1",
    "vue-tsc": "^2.0.26"
  }
}
